{
  "name": "nordnet-component-kit-documentation",
  "version": "0.0.1",
  "description": "Documentation for nordnet-component-kit",
  "config": {
    "dist": "./dist",
    "dev_server": {
      "host": "localhost",
      "port": 9000
    },
    "api_server": {
      "host": "localhost",
      "port": 9002
    }
  },
  "main": "dist/index.html",
  "scripts": {
    "env:prod": "npm config set nordnet-component-kit-documentation:node_env=production",
    "env:dev": "npm config set nordnet-component-kit-documentation:node_env=development",
    "prestart": "npm run env:dev",
    "start": "npm run start:webpack-dev-server",
    "clean": "rimraf dist",
    "build": "npm-run-all env:dev clean webpack",
    "copy": "cp -r lib/. dist/.",
    "webpack": "webpack --colors --progress --config ./webpack/config",
    "start:webpack-dev-server": "node ./webpack/webpack-dev-server",
    "pages": "NODE_ENV=production npm run build && npm run deploy",
    "deploy": "git-directory-deploy --directory ./dist --branch gh-pages"
  },
  "author": "Andreas Brostr√∂m <andreas.brostrom@nordnet.se>",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.3",
    "lodash.kebabcase": "4.0.0",
    "nordnet-component-kit": "0.0.1",
    "normalize.css": "4.1.1",
    "react": "15.0.1",
    "react-addons-pure-render-mixin": "15.0.1",
    "react-bem-grid": "0.0.10",
    "react-dom": "15.0.1",
    "react-prism": "eddywashere/react-prism#15",
    "react-pure-render": "1.0.2"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "css-loader": "0.23.1",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-config-nordnet": "1.1.1",
    "eslint-plugin-jsx-a11y": "0.6.2",
    "eslint-plugin-react": "4.3.0",
    "node-sass": "3.4.2",
    "npm-run-all": "1.7.0",
    "opn": "4.0.1",
    "postcss-loader": "0.8.2",
    "react-addons-test-utils": "15.0.1",
    "react-hot-loader": "1.3.0",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  }
}
