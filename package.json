{
  "name": "nordnet-component-kit",
  "version": "0.0.1",
  "description": "Custom formats used by Nordnet",
  "main": "lib/nordnet-component-kit.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf lib",
    "test": "mocha",
    "test:tdd": "npm run test -- --watch",
    "lint": "eslint --ext=.jsx --ext=.js src test",
    "build": "NODE_ENV=production npm-run-all clean webpack",
    "prebuild:watch": "npm run clean",
    "build:watch": "NODE_ENV=development npm run webpack -- --watch",
    "webpack": "webpack --progress --config webpack/config.babel.js",
    "prepush": "npm-run-all lint test",
    "styleguide-server": "npm run styleguide -- server",
    "styleguide-build": "npm run styleguide -- build",
    "styleguide": "styleguidist --config styleguide/config.js"
  },
  "keywords": [
    "nordnet",
    "react",
    "formatter",
    "intl"
  ],
  "author": "Andreas Brostr√∂m <andreas.brostrom@nordnet.se>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-intl": "^2.0.0"
  },
  "dependencies": {
    "classnames": "2.2.3"
  },
  "devDependencies": {
    "babel-cli": "6.7.7",
    "babel-register": "6.7.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "chai": "3.5.0",
    "chai-enzyme": "0.4.2",
    "cheerio": "0.20.0",
    "css-loader": "0.23.1",
    "dirty-chai": "1.2.2",
    "enzyme": "2.2.0",
    "eslint": "2.8.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-config-nordnet": "1.1.1",
    "eslint-plugin-jsx-a11y": "0.6.2",
    "eslint-plugin-react": "4.3.0",
    "husky": "0.11.4",
    "jsdom": "8.4.0",
    "lodash.camelcase": "4.1.1",
    "mocha": "2.4.5",
    "node-sass": "3.5.1",
    "npm-run-all": "1.7.0",
    "postcss-loader": "0.8.2",
    "react-addons-test-utils": "15.0.1",
    "react-styleguidist": "2.2.0",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "sinon": "1.17.3",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.0"
  }
}
