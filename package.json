{
  "name": "nordnet-formatter",
  "version": "0.0.1",
  "description": "Custom formats used by Nordnet",
  "main": "dist/nordnet-formatter.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf dist",
    "test": "mocha",
    "lint": "eslint --ext=.jsx --ext=.js src test",
    "build": "npm-run-all clean webpack",
    "prebuild:watch": "npm run clean",
    "build:watch": "npm run webpack -- --watch",
    "webpack": "NODE_ENV=production webpack --progress --config webpack.config.babel.js",
    "prepush": "npm-run-all lint test"
  },
  "keywords": [
    "nordnet",
    "react",
    "formatter",
    "intl"
  ],
  "author": "Andreas Brostr√∂m <andreas.brostrom@nordnet.se>",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.3",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-intl": "2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-config-nordnet": "1.1.1",
    "eslint-plugin-jsx-a11y": "0.6.2",
    "eslint-plugin-react": "4.3.0",
    "husky": "0.11.4",
    "mocha": "2.4.5",
    "node-sass": "3.4.2",
    "npm-run-all": "1.7.0",
    "postcss-loader": "0.8.2",
    "react-addons-test-utils": "15.0.1",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "webpack": "1.12.14",
    "webpack-node-externals": "1.0.0"
  }
}
